FROM openjdk:17
COPY target/Weather-0.0.1-SNAPSHOT.jar /app/Weather-0.0.1-SNAPSHOT.jar
COPY src/main/resources/file1.csv /app/file1.csv
COPY src/main/resources/file2.csv /app/file2.csv
COPY src/main/resources/file3.csv /app/file3.csv

WORKDIR /app
ENTRYPOINT ["java", "-jar", "Weather-0.0.1-SNAPSHOT.jar"]
